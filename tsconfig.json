{
    "compilerOptions": {
        "sourceMap": true,
        "target": "es3",
        "moduleResolution": "node",
        "strict": true,
        "baseUrl": ".",
        "outDir": "dist",
        "removeComments": true,
        "downlevelIteration": true,
        "resolveJsonModule": true,
        "lib": ["ES2021.String"]
    },
    "include": ["src/**/*.ts"],
    "exclude": ["node_modules"]
}
